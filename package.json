{
  "name": "blog-service",
  "version": "1.0.0",
  "description": "A backend for persist and manipulate Posts to use in a blog.",
  "main": "./src/index.ts",
  "scripts": {
    "nodemon": "nodemon",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node -r tsconfig-paths/register ./src/index.ts\"",
    "dev": "npm run generate && ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "seed": "ts-node prisma/seed.ts",
    "generate": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "Leo Yassuda",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.2",
    "@types/uuid": "^9.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
