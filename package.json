{
  "name": "blog-service",
  "version": "1.0.0",
  "description": "A backend for persist and manipulate Posts to use in a blog.",
  "main": "./src/index.ts",
  "scripts": {
    "nodemon": "nodemon",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node -r tsconfig-paths/register ./src/index.ts\"",
    "dev": "npm run generate && ts-node -r tsconfig-paths/register ./src/index.ts",
    "clean": "rimraf ./dist",
    "build": "tsc -p .",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "start:prod": "TS_NODE_BASEURL=./dist node -r ts-node/register -r tsconfig-paths/register ./dist/index.js",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "test": "jest"
  },
  "author": "Leo Yassuda",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.3.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "bson": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.2",
    "@types/uuid": "^9.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.0",
    "prisma-erd-generator-markdown": "^1.3.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
